pipeline {
    agent any

    stages {
        stage('Project Build') {
            steps {
                sh 'docker build -t divyarg0013/heyy-nodejsexp:0.0.1.RELEASE .'
            }
        }
        stage('Run Container') {
            steps {
                sh 'docker run -d  divyarg0013/heyy-nodejsexp:0.0.1.RELEASE'
            }
        }
    }
}
